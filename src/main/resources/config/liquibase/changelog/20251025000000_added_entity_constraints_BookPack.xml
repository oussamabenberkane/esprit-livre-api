<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the entity constraints for BookPack.
    -->
    <changeSet id="20251025000000-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="books_id"
                                 baseTableName="rel_book_pack__books"
                                 constraintName="fk_rel_book_pack__books__books_id"
                                 referencedColumnNames="id"
                                 referencedTableName="book"/>

        <addForeignKeyConstraint baseColumnNames="book_pack_id"
                                 baseTableName="rel_book_pack__books"
                                 constraintName="fk_rel_book_pack__books__book_pack_id"
                                 referencedColumnNames="id"
                                 referencedTableName="book_pack"/>
    </changeSet>
</databaseChangeLog>
